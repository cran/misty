\ErrorTok{>}\StringTok{ }\KeywordTok{ci.mean}\NormalTok{(dat[, }\KeywordTok{c}\NormalTok{(}\StringTok{"x1"}\NormalTok{, }\StringTok{"x2"}\NormalTok{, }\StringTok{"x3"}\NormalTok{)], }\DataTypeTok{group =}\NormalTok{ dat}\OperatorTok{$}\NormalTok{group1, }\DataTypeTok{split =}\NormalTok{ dat}\OperatorTok{$}\NormalTok{group2)}
